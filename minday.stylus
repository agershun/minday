//=======================
// CSS стили
//=======================
.cssrect
	//

.cssrrect
	border-radius: 10px

.cssoval
	border-radius:50%

//=======================
// Разные колауты
//=======================
[class*="callout-"]
	position:relative
	background-color: Blue
	@extend .cssrrect	

.aftercallout
	position:absolute
	content:' '
	width: 0
	height: 0
	border: 10px solid transparent

.callout-up:after
	@extends .aftercallout
	border-bottom: 10px solid Blue
	top:0%
	left: 50%
	margin-left: -10px
	margin-top: -20px

.callout-down:after
	@extends .aftercallout
	border-top: 10px solid Blue
	top:100%
	left: 50%
	margin-left: -10px

.callout-left
	padding-left: 10px

.callout-left:after
	@extends .aftercallout
	border-right: 10px solid Blue
	top:50%
	left: 0%
	margin-left: -20px
	margin-top: -10px

.callout-right
	padding-right: 10px

.callout-right:after
	@extends .aftercallout
	border-left: 10px solid Blue
	top:50%
	left: 100%
	margin-top: -10px

