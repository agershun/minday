.sticker-bag1:before
	content: ''
	z-index: -1
	position:absolute
	left: 10px
	bottom: 10px
	height: 55%
	width: 70%
	max-width: 300px
	max-height: 100px
	box-shadow: 0px 8px 16px rgba(0,0,0,0.3)
	-webkit-transform: skew(-15deg) rotate(-10deg)

.sticker-bag1:after
	z-index: -1
	content: ''
	position:absolute
	height: 100%
	width: 70%
	box-shadow: 0px 8px 16px rgba(0,0,0,0.5)
	-webkit-transform: skew(-15deg) rotate(-10deg)
	right: 10px
	bottom: 10px


